<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @challenge.title %>
</p>

<p>
  <strong>Desc:</strong>
  <%= @challenge.desc %>
</p>

<p>
  <strong>Points:</strong>
  <%= @challenge.points %>
</p>

<%= form_for @challenge do |f| %>
  <div class="field">
    <%= f.label :precode %><br>
    <%= f.text_area :precode %>
  </div>
<% end %>

<%= link_to 'Edit', edit_challenge_path(@challenge) %> |
<%= link_to 'Back', challenges_path %>
